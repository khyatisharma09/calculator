    private void zeroActionPerformed(java.awt.event.ActionEvent evt) {                                     
         display.setText(display.getText()+zero.getText());
    }                                    

    private void clearActionPerformed(java.awt.event.ActionEvent evt) {                                      
        display.setText(" ");
        decimalclick= true;
                
                
    }                                     

    private void decimalActionPerformed(java.awt.event.ActionEvent evt) {                                        
        if(decimalclick)
        {
        display.setText(display.getText()+decimal.getText());
       decimalclick=false;
        }
    }                                       

    private void plusminusActionPerformed(java.awt.event.ActionEvent evt) {                                          
        if(decimalclick){
            int value =Integer.parseInt(display.getText());
            value = value * (-1);
            display.setText(String.valueOf(value));
        }
        else
        {
            double value = Double.parseDouble(display.getText());
            value = value * (-1);
            display.setText(String.valueOf(value));
        }
            
            
    }                                         

    private void plusActionPerformed(java.awt.event.ActionEvent evt) {                                     
        a= Double.parseDouble(display.getText());
        display.setText("");
        decimalclick = true;
        plusclick =true;
    }                                    

    private void minusActionPerformed(java.awt.event.ActionEvent evt) {                                      
       a= Double.parseDouble(display.getText());
        display.setText("");
        decimalclick = true;
         minusclick =true;  
    }                                     

    private void multiplyActionPerformed(java.awt.event.ActionEvent evt) {                                         
        a= Double.parseDouble(display.getText());
        display.setText("");
        decimalclick = true;
       multiplyclick =true;
    }                                        

    private void divideActionPerformed(java.awt.event.ActionEvent evt) {                                       
         a= Double.parseDouble(display.getText());
        display.setText("");
        decimalclick = true;
        divideclick =true;
    }                                      

    private void equalsActionPerformed(java.awt.event.ActionEvent evt) {                                       
       b = Double.parseDouble(display.getText());
       if(plusclick)
       {
           c=a+b;
           display.setText(String.valueOf(c));
           plusclick = false;
       }
       else if(minusclick)
       {
           c = a-b;
           display.setText(String.valueOf(c));
           minusclick = false;
       }
       else if(multiplyclick)
       {
           c = a*b;
           display.setText(String.valueOf(c));
           multiplyclick = false;  
       }
       else if(divideclick)
               {
           c =a/b;
           display.setText(String.valueOf(c));
           divideclick = false;
               }
    }                                      

